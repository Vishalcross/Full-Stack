{
    "name": "confusion-server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www"
    },
    "dependencies": {
        "constantinople": "^3.1.1",
        "cookie-parser": "~1.4.4",
        "core-js": "^3.6.5",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "jade": "~1.11.0",
        "jstransformer": "^1.0.0",
        "mongoose": "^5.9.29",
        "mongoose-currency": "^0.2.0",
        "morgan": "~1.9.1",
        "pug": "^3.0.0"
    },
    "prettier": {
        "editor.formatOnSave": true,
        "prettier.useEditorConfig": true,
        "trailingComma": "none",
        "singleQuote": false,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "quoteProps": "as-needed",
        "jsxSingleQuote": false,
        "jsxBracketSameLine": false,
        "bracketSpacing": true,
        "arrowParens": "always"
